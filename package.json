{
  "name": "bar-lobby",
  "version": "0.8.4",
  "private": true,
  "description": "Lobby client for the RTS game Beyond All Reason",
  "author": "Jazcash",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyond-all-reason/bar-lobby.git"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run clean-dist && npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "clean-dist": "rimraf dist",
    "lint-check": "eslint . --ext .ts,.vue",
    "lint": "eslint . --ext .ts,.vue --fix",
    "format-check": "prettier --check src/**/*.{ts,vue,scss,json}",
    "format": "prettier --write src/**/*.{ts,vue,scss,json}",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preversion": "npm run lint-check && npm run format-check",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.23.5",
    "electron-devtools-installer": "^3.1.0",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "^4.6.1",
    "vue": "^3.2.37",
    "ajv": "^8.7.1",
    "tachyon-client": "^3.12.2",
    "axios": "^0.24.0",
    "octokit": "^1.7.1",
    "playwright": "^1.25.1",
    "sdfz-demo-parser": "^4.7.0",
    "spring-map-parser": "^4.3.5",
    "bar-db": "^6.3.0",
    "date-fns": "^2.29.1",
    "flag-icons": "^6.1.1",
    "howler": "^2.2.3",
    "jaz-ts-utils": "^4.2.0",
    "luaparse": "^0.3.1",
    "node-7z": "^3.0.0",
    "node-json-db": "^1.6.0",
    "primevue": "^3.16.2",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "vite-plugin-esmodule": "^1.4.2",
    "vitest": "^0.22.1",
    "@iconify-icons/mdi": "^1.2.14",
    "@iconify/json": "^1.1.420",
    "@iconify/vue": "^3.2.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/howler": "^2.2.4",
    "@types/luaparse": "^0.2.8",
    "@types/node-7z": "^2.1.4",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/cli": "^5.0.0-rc.2",
    "@vue/cli-plugin-router": "^5.0.0-rc.2",
    "@vue/cli-plugin-typescript": "^5.0.0-rc.2",
    "@vue/cli-service": "^5.0.0-rc.2",
    "@vue/compiler-sfc": "^3.2.29",
    "@vueuse/core": "^7.3.0",
    "cross-env": "^7.0.3",
    "electron": "^16.2.6",
    "glob-promise": "^4.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "tsconfig-paths": "^3.12.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2",
    "vite": "^3.0.9",
    "vite-plugin-commonjs-externals": "^0.1.1",
    "vite-plugin-electron": "^0.9.2",
    "vite-plugin-electron-renderer": "^0.9.0",
    "vue-auto-routing": "^1.0.1",
    "vue-cli-plugin-auto-routing": "^1.3.0",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^0.40.5",
    "vue3-markdown-it": "^1.0.9",
    "vue3-popper": "^1.4.1",
    "worker-plugin": "^5.0.1"
  }
}
