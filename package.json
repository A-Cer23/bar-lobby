    "axios": "^0.24.0",
    "bar-db": "^6.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "electron": "^16.2.6",
    "electron-devtools-installer": "^3.1.0",
    "electron-unhandled": "^3.0.2",
